{
  "lineWidth": 80,
  "indentWidth": 2,
  "typescript": {
    "quoteStyle": "alwaysDouble",
    "semiColons": "asi",
    "quoteProps": "asNeeded",
    "useBraces": "always"
  },
  "json": {
  },
  "markdown": {
  },
  "toml": {
  },
  "includes": [
    "**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,toml}",
    "**/Dockerfile"
  ],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.71.1.wasm",
    "https://plugins.dprint.dev/json-0.15.3.wasm",
    "https://plugins.dprint.dev/markdown-0.13.3.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/dockerfile-0.2.3.wasm"
  ]
}